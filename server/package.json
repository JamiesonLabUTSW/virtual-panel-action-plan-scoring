{
  "name": "@grading/server",
  "version": "0.0.0",
  "description": "Express server with CopilotKit runtime for LLM-as-a-judge grading pipeline",
  "private": true,
  "type": "module",
  "engines": {
    "node": ">=24"
  },
  "scripts": {
    "clean": "rm -rf node_modules dist",
    "dev": "tsx watch src/index.ts",
    "build": "tsup",
    "type-check": "tsc --noEmit",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:coverage": "vitest run --coverage",
    "test:related": "vitest related --run"
  },
  "dependencies": {
    "@ag-ui/client": "^0.0.43",
    "@copilotkit/runtime": "^1.51.0",
    "@langchain/core": "^1.1.0",
    "@langchain/openai": "^1.2.0",
    "express": "^5.0.0",
    "express-rate-limit": "^8.0.0",
    "openai": "^6.0.0",
    "rxjs": "^7.8.0",
    "zod": "^4.0.0"
  },
  "devDependencies": {
    "@types/express": "^5.0.0",
    "@types/node": "^20.0.0",
    "@types/supertest": "^6.0.3",
    "@vitest/coverage-v8": "^1.0.0",
    "supertest": "^7.2.2",
    "tsup": "^8.0.0",
    "tsx": "^4.0.0",
    "typescript": "^5.6.0",
    "vitest": "^1.0.0"
  }
}
